{
  "name": "exorbitant",
  "version": "0.0.18",
  "description": "Web calculator library",
  "main": "dist/exorbitant.umd.js",
  "module": "source/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajsite/exorbitant.git"
  },
  "author": "Milan Raj (@rajsite)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rajsite/exorbitant/issues"
  },
  "homepage": "https://github.com/rajsite/exorbitant#readme",
  "files": [
    "dist/*",
    "source/*.js"
  ],
  "scripts": {
    "build": "make build",
    "build:development": "make build CONFIGURATION=development",
    "clean": "rimraf dist",
    "rebuild": "npm run clean && npm run build",
    "rebuild:development": "npm run clean && npm run build:development",
    "bundle": "rollup source/index.js --format umd --name exorbitantHelpers --file dist/exorbitant.umd.js",
    "example:node": "node example/node-example.js",
    "wasmer": "wasmer run --disable-cache dist/exorbitant.wasm",
    "example:node:wasi": "node --experimental-wasi-unstable-preview1 --experimental-wasm-bigint example/node-wasi.js"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1"
  }
}
